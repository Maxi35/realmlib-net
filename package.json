{
  "name": "@realmlib/net",
  "version": "4.0.0",
  "description": "A networking library for Realm of the Mad God.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run compile && npm run lint && npm run test",
    "compile": "tsc -p .",
    "lint": "tslint -p .",
    "test": "ava",
    "docs": "typedoc --options ./typedoc.json",
    "index": "tsai --path src --overwrite"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "files": [
    "lib/",
    "license.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomas-crane/realmlib-net.git"
  },
  "keywords": [
    "rotmg",
    "typescript",
    "library",
    "realmlib"
  ],
  "author": "tcrane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomas-crane/realmlib-net/issues"
  },
  "homepage": "https://github.com/thomas-crane/realmlib-net#readme",
  "devDependencies": {
    "@types/node": "^13.1.7",
    "ava": "^2.4.0",
    "ts-node": "^8.6.2",
    "tsai": "^2.1.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.16.7",
    "typescript": "^3.7.5"
  },
  "dependencies": {}
}
